@model IEnumerable<IOITWebApp31.Models.Data.NewsDTO>
@using IOITWebApp31.Models;

@if (Model != null)
{
    var i = 0;
    @foreach (var item in Model)
    {
        <div class="box item-news-4-1-2">
            <div class="box-text">
                <div class="cate-box textLine-1"><a href="@("/" + item.Url)">@item.NameCategory</a></div>
                <h2 class="heading-box">
                    <a href="@("/" + item.Url)" class="textLine-2">
                        @item.Title
                    </a>
                </h2>
            </div>
        </div>
    }
}