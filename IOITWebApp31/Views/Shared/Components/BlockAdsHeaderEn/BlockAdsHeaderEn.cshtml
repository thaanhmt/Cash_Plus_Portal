@model IEnumerable<IOITWebApp31.Models.EF.Slide>
@using IOITWebApp31.Models;

@if (Model != null)
{
    <div class="adsBannerHeader">
       @foreach (var item in Model)
        {
            <a href="@(item.Url)" target="_blank">
                <img src="@(ViewBag.LinkSite)/uploads/@item.Image" alt="Quảng Cáo">
            </a>
        }
    </div>
        }