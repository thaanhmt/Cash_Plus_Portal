@model IEnumerable<IOITWebApp31.Models.EF.News>
@using IOITWebApp31.Models;

@if (Model != null)
{

    @foreach (var item in Model)
    {
        <div class="item-anpham-9-2-3">
            <a href="@("/" + item.Url)" class="d-block"><img class="image-cover" src="@(ViewBag.LinkSite)/uploads/@item.Image" alt="featured image"></a>
        </div>
    }
}